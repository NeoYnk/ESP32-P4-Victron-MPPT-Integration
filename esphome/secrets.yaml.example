# ESPHome Secrets - Copy to secrets.yaml and fill in your values
# DO NOT commit secrets.yaml to git!

# API Encryption Key (generate with: openssl rand -base64 32)
api_encryption_key: "YOUR_32_BYTE_BASE64_KEY_HERE"

# OTA Password
ota_password: "YOUR_OTA_PASSWORD_HERE"

# WiFi credentials (not needed for Ethernet, but useful for fallback)
# wifi_ssid: "YOUR_WIFI_SSID"
# wifi_password: "YOUR_WIFI_PASSWORD"
